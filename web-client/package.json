{
  "name": "web-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "oxlint . && eslint .",
    "fmt": "oxfmt .",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-oxlint": "^1.35.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.20.0",
    "oxlint": "^1.35.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm oxlint",
      "pnpm eslint --max-warnings=0",
      "pnpm oxfmt"
    ],
    "*.{json,css,scss,md,yml,yaml}": [
      "pnpm oxfmt"
    ]
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
